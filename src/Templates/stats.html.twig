{% extends "layout.html.twig" %}

{% block content %}
<div class="container-fluid">
    <div class="card bg-light border-0">
        <h3 class="card-title">Dashboard</h3>
    </div>
    <div class="card-body">
        <h5 class="card-title">Filters</h5>
        {% include "daterangepicker.html.twig" %}
    </div>
    <div class="row">
        <div class="col-sm-3">
            {% include "total_orders.html.twig" %}
        </div>
        <div class="col-sm-3">
            {% include "total_customers.html.twig" %}
        </div>
        <div class="col-sm-3">
            {% include "total_revenue.html.twig" %}
        </div>
    </div>
</div>
{% endblock %}

{% block javascript %}
{% include "daterangepickerjs.html.twig" %}
{% endblock %}
